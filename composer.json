{
    "name": "tiderjian/qs_lara_cmf",
    "description": "Laravel Content Management Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tiderjian",
            "email": "164710115@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "tcg/voyager": "^1.1"
    },
    "autoload": {
        "psr-4": {
